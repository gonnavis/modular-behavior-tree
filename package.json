  },
  "repository": {
    "type": "git",
    "url": "https://github.com/telcy/modular-behavior-tree.git"
  },
  "homepage": "https://github.com/telcy/modular-behavior-tree",
  "author": {
    "name": "Jascha Burmeister",
    "url": "http://www.jaschaburmeister.com"
  },
  "license": "MIT",
  "dependencies": {
    "cross-env": "^7.0.3",
    "xml-parser": "^1.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1"
  }
}
