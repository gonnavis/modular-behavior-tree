{
  "name": "modular-behavior-tree",
  "version": "1.0.3",
  "description": "JavaScript implementation of a modular behavior tree",
  "browser": "./lib/index.js",
  "scripts": {
    "test": "babel-node --presets es2015 example/index.js",
